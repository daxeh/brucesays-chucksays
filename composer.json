{
    "name": "Bruce says, Chuck says",
    "version": "0.0.1",
    "description": "Bruces says what Chuck says, he is no more than just joke double that is built on Chuck Norric Jokes API.",
    "license": "MIT",
    "keywords": [
        "framework",
        "laravel"
    ],
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "cviebrock/laravel-elasticsearch": "^3.0",
        "fideloper/proxy": "^3.3",
        "laravel/framework": "5.5.*",
        "laravel/passport": "^4.0",
        "php-http/client-common": "^1.7",
        "php-http/discovery": "^1.3",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/httplug": "^1.1",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "v2.4.1",
        "laravel/tinker": "~1.0",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "heroku/heroku-buildpack-php": "^127.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "squizlabs/php_codesniffer": "^3.1",
        "pusher/pusher-http-laravel": "^4.0",
        "pusher/pusher-php-server": "^3.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
